{
  "name": "viddey-cdn-redirect",
  "version": "1.0.0",
  "description": "Viddey CDN Redirect with Cloudflare Pages",
  "main": "functions/[[path]].js",
  "scripts": {
    "dev": "wrangler pages dev .",
    "deploy": "wrangler pages deploy . --project-name=viddey-redirect",
    "deploy:prod": "wrangler pages deploy . --project-name=viddey-redirect --branch=main",
    "test": "curl -A \"Twitterbot\" http://localhost:8788/s/test123",
    "test:user": "curl -A \"Chrome\" -I http://localhost:8788/s/test123",
    "lint": "eslint functions/ --fix",
    "format": "prettier --write \"functions/**/*.js\""
  },
  "keywords": ["cdn", "cloudflare", "redirect", "viddey"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "wrangler": "^3.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
